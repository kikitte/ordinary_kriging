  "version": "0.2.0",
  "configurations": [
    {
      "name": "oridinary kriging",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/ordinary_kriging",
      "args": [
        "chap15/stations.txt",
        "2",
        "40000",
        "12",
        "2",
        "5",
        "7",
        "200"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "ordinary kriging",
      "miDebuggerPath": "/usr/bin/gdb"
    }
  ]
}